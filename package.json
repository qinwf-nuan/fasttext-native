{
  "name": "fasttext-native",
  "version": "0.2.1-rc0",
  "description": "fastText bindings for Node.js",
  "main": "index.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "mocha --opts ./mocha.opts ./test"
  },
  "repository": {
