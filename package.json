  "contributors": [
    "Tuan Anh Tran <me@tuananh.org>"
  ],
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "nan": "2.8.0",
    "node-pre-gyp": "0.6.39"
  },
  "devDependencies": {
    "mocha": "4.0.1"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "binary": {
    "module_name": "fasttext",
